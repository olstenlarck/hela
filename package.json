{
  "private": true,
  "name": "hela-monorepo",
  "version": "0.0.0-monorepo",
  "license": "Apache-2.0",
  "bolt": {
    "workspaces": [
      "packages/*"
    ]
  },
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock packages/*/node_modules",
    "fresh": "bolt clean && bolt install",
    "lint": "xaxa packages/*/src packages/*/test"
  },
  "devDependencies": {
    "bolt": "^0.20.7"
  },
  "dependencies": {
    "arrayify": "^1.0.0",
    "asia": "^0.3.3",
    "auto-bind": "^1.2.0",
    "esm": "^3.0.48",
    "execa-pro": "^1.0.3",
    "find-file-up": "^2.0.1",
    "js-yaml": "^3.12.0",
    "json-6": "^1.0.2",
    "mri": "^1.1.1",
    "p-finally": "^1.0.0",
    "p-map": "^1.2.0",
    "p-map-series": "^1.0.0",
    "p-memoize": "^2.0.0",
    "p-reflect": "^1.0.0",
    "redolent": "^2.0.5",
    "sade": "^1.4.1",
    "xaxa": "^0.3.4"
  },
  "nyc": {
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "lines": 0,
    "exclude": [
      "test"
    ],
    "extension": [
      ".js",
      ".jsx",
      ".mjs"
    ]
  }
}
