{
  "name": "@hela/cli",
  "version": "3.0.0",
  "description": "The CLI for running Hela commands",
  "author": "Charlike Mike Reagent <opensource@tunnckocore.com> (https://tunnckocore.com)",
  "homepage": "https://github.com/tunnckoCore/hela",
  "license": "MPL-2.0",
  "licenseStart": 2017,
  "repository": {
    "type": "git",
    "url": "https://github.com/tunnckoCore/hela.git",
    "directory": "modules/cli"
  },
  "engines": {
    "node": ">=10.13"
  },
  "dependencies": {
    "@hela/core": "portal:*",
